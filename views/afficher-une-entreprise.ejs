<!DOCTYPE html>
<html lang="en">
<head>
    <% include ./partials/head %>
</head>
<body>
<% include ./partials/nave %>

<div class="container">

        <div class="col-lg-9">
            <div class="panel-primary">
                <b></b>
                <div class="panel-heading">
                    <div class="panel-title"><h3>Entreprise : <%= company.L1_NORMALISEE %></h3></div>
                </div>
                </b>
                <ul class="list-group">
                    <li class="list-group-item">
                        <div class="row toggle" id="dropdown-detail-1" data-toggle="detail-1">
                            <div class="col-xs-10 mouseover">
                               <h4> Informations générales</h4>
                            </div>
                            <div class="col-xs-2"><i class="fa fa-chevron-down pull-right"></i></div>
                        </div>
                        <div id="detail-1">

                                <table class="table">
                                    <tbody>
                                    <tr>
                                        <td><h5>Numero de SIREN</h5></td>
                                        <td><h5><%= company.SIREN %></h5></td>
                                    </tr>
                                    <tr>
                                        <td><h5>Numero de SIRET</h5></td>
                                        <td><h5><%= company.SIREN + company.NIC %></h5></td>
                                    </tr>
                                    <tr>
                                        <td><h5>Adresse</h5></td>
                                        <td><h5><%= company.L3_NORMALISEE + ' ' + company.L4_NORMALISEE + ' ' + company.L5_NORMALISEE %></h5></td>
                                    </tr>
                                    <tr>
                                        <td><h5>Code Postal</h5></td>
                                        <td><h5><%= company.CODPOS %></h5></td>
                                    </tr>
                                    <tr>
                                        <td><h5>Ville</h5></td>
                                        <td><h5><%= company.LIBCOM %></h5></td>
                                    </tr>
                                    </tbody>
                                </table>
                        </div>
                    </li>
                    <li class="list-group-item">
                        <div class="row toggle" id="dropdown-detail-2" data-toggle="detail-2">
                            <div class="col-xs-10 mouseover">
                                <h4> Informations localisation de l'entreprise</h4>
                            </div>
                            <div class="col-xs-2"><i class="fa fa-chevron-down pull-right"></i></div>
                        </div>
                        <div id="detail-2">
                            <div class="fluid-row">
                                <table class="table table-user-information">
                                    <tbody>
                                    <tr>
                                        <td><h5>Région</h5></td>
                                        <td><h5><%= company.LIBREG %></h5></td>
                                    </tr>
                                    <tr>
                                        <td><h5>Département</h5></td>
                                        <td><h5><%= company.DEPET%></h5></td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </li>
                    <li class="list-group-item">
                        <div class="row toggle" id="dropdown-detail-3" data-toggle="detail-3">
                            <div class="col-xs-10 mouseover" >
                                <h4> Informations économique de l'entreprise</h4>
                            </div>
                            <div class="col-xs-2"><i class="fa fa-chevron-down pull-right"></i></div>
                        </div>
                        <div id="detail-3">
                            <div class="fluid-row">
                                <table class="table table-user-information">
                                    <tbody>
                                    <tr>
                                        <td><h5>Code Naf</h5></td>
                                        <td><h5><%= company.APET700 %></h5></td>
                                    </tr>
                                    <tr>
                                        <td><h5>Activité principale</td>
                                        <td><h5><%= company.LIBAPET %></h5></td>
                                    </tr>
                                    <tr>
                                        <td><h5>Tranche d'effectif</h5></td>
                                        <td><h5><%= company.LIBTEFEN %>  %></h5></td>
                                    </tr>
                                    </tbody>
                                </table>
                            </div>

                        </div>
                    </li>
                </ul>
            </div>
        </div>

</div>

<% include ./partials/scripts %>
<% include ./partials/footer %>

</body>
</html>